<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.personalbudget.controller.PrimaryController">
   <children>
      <HBox alignment="CENTER" spacing="10.0" stylesheets="@style.css">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <children>
            <HBox fx:id="titleHolder" alignment="CENTER">
               <children>
                  <Label text="20">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <styleClass>
                        <String fx:value="nav-title" />
                        <String fx:value="clickable" />
                     </styleClass>
                  </Label>
                  <Label styleClass="nav-title" text=" " />
                  <Label text="September">
                     <styleClass>
                        <String fx:value="nav-title" />
                        <String fx:value="clickable" />
                     </styleClass>
                  </Label>
                  <Label styleClass="nav-title" text=" " />
                  <Label text="2024">
                     <styleClass>
                        <String fx:value="nav-title" />
                        <String fx:value="clickable" />
                     </styleClass>
                  </Label>
                  <Pane HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </HBox>
      <Pane fx:id="contentPane" VBox.vgrow="ALWAYS" />
      <HBox spacing="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Pane HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" text="Add Income" />
            <Button mnemonicParsing="false" text="Add Expense" />
         </children>
      </HBox>
   </children>
</VBox>
