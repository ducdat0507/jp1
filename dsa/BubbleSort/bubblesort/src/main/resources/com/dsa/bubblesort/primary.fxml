<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dsa.bubblesort.PrimaryController">
   <children>
      <TableView editable="true" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn prefWidth="112.0" text="Name" />
          <TableColumn prefWidth="88.0" text="Birthdate" />
        </columns>
      </TableView>
      <HBox alignment="TOP_RIGHT">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <children>
            <Button mnemonicParsing="false" text="Sort" />
         </children>
      </HBox>
   </children>
</VBox>
